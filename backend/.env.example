# PostgreSQL - para Supabase use pooled (6543) e direct (5432)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/energy_invoices"
DIRECT_URL="postgresql://postgres:postgres@localhost:5432/energy_invoices"

# Storage: minio (local) | supabase (produção)
STORAGE_PROVIDER=minio

# MinIO (quando STORAGE_PROVIDER=minio)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=invoices
S3_REGION=us-east-1

# Supabase (quando STORAGE_PROVIDER=supabase)
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_SERVICE_KEY=sua-service-role-key
SUPABASE_BUCKET=invoices
